 *
 */
public class GoodsHomepageCategoryPojo implements Serializable{
	private Integer goodsHomepageCategoryId;
	private String homepageCategoryName;
	private String homepageCategoryImg;
	private Integer goodsHomepageCategorySort;
	private GoodsHomepageCategory goodsHomepageCategory;
	List<GoodsPojo> goodsList;
	public GoodsHomepageCategoryPojo() {
