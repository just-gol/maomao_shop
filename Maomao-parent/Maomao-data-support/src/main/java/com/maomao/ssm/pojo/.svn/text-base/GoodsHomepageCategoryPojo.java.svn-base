		super();
		this.goodsHomepageCategoryId = goodsHomepageCategoryId;
		this.homepageCategoryName = homepageCategoryName;
		this.homepageCategoryImg = homepageCategoryImg;
		this.goodsHomepageCategorySort = goodsHomepageCategorySort;
		this.goodsHomepageCategory = goodsHomepageCategory;
