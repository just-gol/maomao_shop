package com.maomao.ssm.pojo.home;

import java.io.Serializable;
import java.util.List;

/** 
* @author:wzy
* @descrption:首页内容详情
* @version:
*/

public class HomeDetails implements Serializable{

	private List<HomeBannerDetail> homeBannerList;//首页banner
	private List<CategoryDetail> homeCategoryList;//首页分类
	private List<RecommendDetail> homeRecommendList;//首页推荐位内容
	
	public List<HomeBannerDetail> getHomeBannerList() {
		return homeBannerList;
	}
	public void setHomeBannerList(List<HomeBannerDetail> homeBannerList) {
		this.homeBannerList = homeBannerList;
	}
	public List<CategoryDetail> getHomeCategoryList() {
		return homeCategoryList;
	}
	public void setHomeCategoryList(List<CategoryDetail> homeCategoryList) {
		this.homeCategoryList = homeCategoryList;
	}
	public List<RecommendDetail> getHomeRecommendList() {
		return homeRecommendList;
	}
	public void setHomeRecommendList(List<RecommendDetail> homeRecommendList) {
		this.homeRecommendList = homeRecommendList;
	}
	
	
}
































