	public static String getPayTypeString(Byte payType) {
		if (payType == null) {
			return null;
		}
		switch (payType) {
		case 0:
			return "在线支付";
		case 1:
			return "货到付款";
		default:
			return null;
		}
	}

	public static String getGetWayString(Byte getWay) {
		if (getWay == null) {
			return null;
		}
		switch (getWay) {
		case 0:
			return "快递";
		case 1:
			return "自取";
		default:
			return null;
		}
	}

	public static String getWarehouseTypeString(Byte warehouseType) {
		if (warehouseType == null) {
